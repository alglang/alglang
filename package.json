{
    "private": true,
    "scripts": {
        "dev": "yarn development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "development-quiet": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --display=none --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "yarn development --watch",
        "watch-poll": "yarn watch --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --disable-host-check --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "yarn production",
        "build": "yarn production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "mocha": "cross-env NODE_ENV=testing mochapack --webpack-config=node_modules/laravel-mix/setup/webpack.config.js --require tests/JavaScript/setup.js tests/JavaScript/\\*\\*/\\*.spec.js",
        "mocha-watch": "yarn mocha --watch",
        "test": "cross-env NODE_ENV=testing nyc yarn mocha",
        "lint": "eslint --ext .js,.vue resources/js tests/JavaScript",
        "serve": "php artisan serve",
        "start": "concurrently --kill-others \"yarn serve --quiet\" \"yarn development-quiet --watch\" \"yarn mocha-watch\""
    },
    "devDependencies": {
        "@tailwindcss/custom-forms": "^0.2",
        "@testing-library/vue": "^5.0.4",
        "@vue/babel-preset-app": "^4.4.6",
        "babel-loader": "^8.1.0",
        "babel-plugin-istanbul": "^6.0.0",
        "chai": "^4.2.0",
        "chai-dom": "^1.8.2",
        "concurrently": "^5.2.0",
        "cross-env": "^7.0",
        "eslint": "^7.3.1",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-plugin-chai-friendly": "^0.6.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-vue": "^6.2.2",
        "jsdom": "^16.2.2",
        "jsdom-global": "^3.0.2",
        "laravel-mix": "^5.0.1",
        "laravel-mix-merge-manifest": "^0.1.2",
        "mocha": "^8.0.1",
        "mochapack": "^2.0.0",
        "moxios": "^0.4.0",
        "nyc": "^15.1.0",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.15.2",
        "sass-loader": "^8.0.0",
        "tailwindcss": "^1.4",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "axios": "^0.19",
        "eslint-plugin-mocha": "^7.0.1",
        "leaflet": "^1.6.0",
        "leaflet-defaulticon-compatibility": "^0.1.1",
        "lodash": "^4.17.19",
        "turbolinks": "^5.2.0",
        "vue": "^2.6.11",
        "vue-turbolinks": "^2.1.0",
        "vue2-leaflet": "^2.5.2"
    }
}
