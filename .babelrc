{
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "targets": {
            "node": "current"
          }
        }]
      ]
    },
    "development": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false,
          "targets": {
            "browsers": "> 2%"
          }
        }]
      ]
    },
    "production": {
      "presets": [
        ["@babel/preset-env", {
          "modules": false,
          "forceAllTransforms": true,
          "targets": {
            "browsers": "> 2%"
          }
        }]
      ]
    }
  }
}
